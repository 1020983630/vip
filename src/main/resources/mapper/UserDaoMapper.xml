<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.4//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.vip.dao.UserDao">
    <select id="selectByAccount" resultType="com.example.vip.entity.UserEntity">
        select * from sys_user where account = #{account}
    </select>
</mapper>